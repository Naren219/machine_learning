   "source": [
    "filename = 't10k-labels-idx1-ubyte.gz'\n",
    "num_images = 5\n",
    "print('Extracting', filename)\n",
    "with gzip.open(filename) as bytestream:\n",
    "    bytestream.read(8)\n",
    "    buf = bytestream.read(1 * num_images)\n",
    "    labels = np.frombuffer(buf, dtype=np.uint8).astype(np.int64)\n",
    "return labels"
   ]
  }
